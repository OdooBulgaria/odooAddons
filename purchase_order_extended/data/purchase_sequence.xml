<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for purchase.order --> 
        <record forcecreate="1" id="seq_type_purchase_" model="ir.sequence.type">
            <field name="name">Purchase Order RFQ</field>
            <field name="code">purchase.order.rfq</field>
        </record>
        <record forcecreate="1" id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order RFQ</field>
            <field name="code">purchase.order.rfq</field>
            <field name="prefix">PR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
	<record forcecreate="1" id="seq_type_purchase_order" model="ir.sequence.type">
            <field name="name">Purchase Order Bid</field>
            <field name="code">purchase.order.bid</field>
        </record>
        <record forcecreate="1" id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order Bid</field>
            <field name="code">purchase.order.bid</field>
            <field name="prefix">PB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
